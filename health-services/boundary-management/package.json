  "private": true,
  "scripts": {
    "build": "yarn run build-ts",
    "build-ts": "tsc",
    "clean": "rm -rf ./dist",
    "serve": "if [ \"$DEBUG\" = \"true\" ]; then NODE_OPTIONS=--max-old-space-size=1024 node --inspect=0.0.0.0:9229 dist/index.js; else NODE_OPTIONS=--max-old-space-size=1024 node dist/index.js; fi",
    "start": "yarn run dev",
    "test": "jest",
    "dev": "ts-node-dev --respawn src/server/index.ts",
    "prod": "if [ \"$DEBUG\" = \"true\" ]; then cp tsconfig.debug.json tsconfig.json; fi && yarn build && yarn serve",
    "watch-ts": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egovernments/health-campaign-services.git"
  },
